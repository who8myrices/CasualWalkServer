<?xml version="1.0" encoding="UTF-8"?>	
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	
	 xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/
javaee/web-app_2_5.xsd"	
	 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/
javaee/web-app_3_0.xsd"	
	 id="WebApp_ID" version="3.0">	
	 <display-name>Tiny-Jetty-App</display-name>	
	
	 <servlet>	
	 	<servlet-name>First</servlet-name>	
	 	<servlet-class>edu.vt.ece4564.example.HelloWorldServlet</servlet-class>	
	 </servlet>	
	
	 	 <servlet>	
	 	<servlet-name>api</servlet-name>	
	 	<servlet-class>edu.vt.ece4564.example.apiServlet</servlet-class>	
	 </servlet>	
	 
	 	 <servlet>	
	 	<servlet-name>data</servlet-name>	
	 	<servlet-class>edu.vt.ece4564.example.dataServlet</servlet-class>	
	 </servlet>	
	 <servlet-mapping>	
	 	<servlet-name>First</servlet-name>	
	 	<url-pattern>/</url-pattern>	
	 	
	 </servlet-mapping>	
	 	 <servlet-mapping>	
	 	<servlet-name>api</servlet-name>	
	 	<url-pattern>/api</url-pattern>	
	 </servlet-mapping>	
	 
	 	 <servlet-mapping>	
	 	<servlet-name>data</servlet-name>	
	 	<url-pattern>/data</url-pattern>	
	 </servlet-mapping>	
	
</web-app>	
